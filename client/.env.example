# Environment Configuration
# Copy this file to .env and fill in your values

# Node Environment
NODE_ENV=development

# API Configuration
API_URL=http://localhost:8080
IDENTITY_SERVICE_URL=http://localhost:5050
ORDER_SERVICE_URL=http://localhost:8080
PAYMENT_SERVICE_URL=http://localhost:50051

# Session Configuration
SESSION_SECRET=your-super-secret-session-key-change-this-in-production

# Application Configuration
APP_NAME=WebApp
APP_URL=http://localhost:3000

# Database (if needed for frontend sessions)
# DATABASE_URL=postgresql://user:password@localhost:5432/webapp_db

# Third-party Services (examples)
# SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id
# GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX

# Feature Flags
# ENABLE_ANALYTICS=true
# ENABLE_ERROR_TRACKING=true
